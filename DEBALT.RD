000020 READ-DEBALT.
000030	     READ DEBALT WITH IGNORE LOCK
	       KEY IS DAL-KEY.
000040     IF WS-STATUS = "00"
               MOVE ZERO         TO WS-F-ERROR
000050	       GO TO READ-DEBALT-EXIT.
           IF WS-STATUS = "23"
	       MOVE 47		 TO WS-F-ERROR
000050	       GO TO READ-DEBALT-EXIT.
000060     IF WS-STAT1 = "2" OR "3" OR "4"
000070	       MOVE 47		 TO WS-F-ERROR
000080         PERFORM READ-ERROR.
	     GO TO READ-DEBALT-EXIT.

000150 READ-DEBALT-LOCK.
000160	     READ DEBALT WITH KEPT LOCK
	       KEY IS DAL-KEY.
000170     IF WS-STATUS = "00"
               MOVE ZERO         TO WS-F-ERROR
000180	       GO TO READ-DEBALT-EXIT.
           IF WS-STATUS = "23"
	       MOVE 47		 TO WS-F-ERROR
000050	       GO TO READ-DEBALT-EXIT.
000190     IF WS-STAT1 = "2" OR "3" OR "4"
000200	       MOVE 47		 TO WS-F-ERROR
000210         PERFORM READ-ERROR.
000220     IF RECORD-LOCKED
	       MOVE W02-DEBALT	 TO WS-FILE
	       MOVE ZERO	 TO WS-KEY
000230         PERFORM LOCKED-RECORD
000240	       GO TO READ-DEBALT-LOCK.
000250     IF TOO-MANY-LOCKS
000260	       UNLOCK DEBALT
000270	       GO TO READ-DEBALT-LOCK.
	     GO TO READ-DEBALT-EXIT.

