000020 READ-DEBPRF.
000030	     READ DEBPRF WITH IGNORE LOCK
	       KEY IS DBP-AC.
000040     IF WS-STATUS = "00"
               MOVE ZERO         TO WS-F-ERROR
000050	       GO TO READ-DEBPRF-EXIT.
           IF WS-STATUS = "23"
	       MOVE 56		 TO WS-F-ERROR
000050	       GO TO READ-DEBPRF-EXIT.
000060     IF WS-STAT1 = "2" OR "3" OR "4"
000070	       MOVE 56		 TO WS-F-ERROR
000080         PERFORM READ-ERROR.
	     GO TO READ-DEBPRF-EXIT.

000150 READ-DEBPRF-LOCK.
000160	     READ DEBPRF WITH KEPT LOCK
	       KEY IS DBP-AC.
000170     IF WS-STATUS = "00"
               MOVE ZERO         TO WS-F-ERROR
000180	       GO TO READ-DEBPRF-EXIT.
           IF WS-STATUS = "23"
	       MOVE 56		 TO WS-F-ERROR
000050	       GO TO READ-DEBPRF-EXIT.
000190     IF WS-STAT1 = "2" OR "3" OR "4"
000200	       MOVE 56		 TO WS-F-ERROR
000210         PERFORM READ-ERROR.
000220     IF RECORD-LOCKED
	       MOVE W02-DEBPRF	 TO WS-FILE
	       MOVE ZERO	 TO WS-KEY
000230         PERFORM LOCKED-RECORD
000240	       GO TO READ-DEBPRF-LOCK.
000250     IF TOO-MANY-LOCKS
000260	       UNLOCK DEBPRF
000270	       GO TO READ-DEBPRF-LOCK.
	     GO TO READ-DEBPRF-EXIT.

