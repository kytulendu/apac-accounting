000020 READ-DEBHIS.
000030	     READ DEBHIS WITH IGNORE LOCK
	       KEY IS DHS-KEY.
000040     IF WS-STATUS = "00"
               MOVE ZERO         TO WS-F-ERROR
000050	       GO TO READ-DEBHIS-EXIT.
           IF WS-STATUS = "23"
	       MOVE 54		 TO WS-F-ERROR
000050	       GO TO READ-DEBHIS-EXIT.
000060     IF WS-STAT1 = "2" OR "3" OR "4"
000070	       MOVE 54		 TO WS-F-ERROR
000080         PERFORM READ-ERROR.
	     GO TO READ-DEBHIS-EXIT.

000150 READ-DEBHIS-LOCK.
000160	     READ DEBHIS WITH KEPT LOCK
	       KEY IS DHS-KEY.
000170     IF WS-STATUS = "00"
               MOVE ZERO         TO WS-F-ERROR
000180	       GO TO READ-DEBHIS-EXIT.
           IF WS-STATUS = "23"
	       MOVE 54		 TO WS-F-ERROR
000050	       GO TO READ-DEBHIS-EXIT.
000190     IF WS-STAT1 = "2" OR "3" OR "4"
000200	       MOVE 54		 TO WS-F-ERROR
000210         PERFORM READ-ERROR.
000220     IF RECORD-LOCKED
	       MOVE W02-DEBHIS	 TO WS-FILE
	       MOVE ZERO	 TO WS-KEY
000230         PERFORM LOCKED-RECORD
000240	       GO TO READ-DEBHIS-LOCK.
000250     IF TOO-MANY-LOCKS
000260	       UNLOCK DEBHIS
000270	       GO TO READ-DEBHIS-LOCK.
	     GO TO READ-DEBHIS-EXIT.

000310 READ-DEBHIS-NEXT.
000320	     READ DEBHIS NEXT WITH IGNORE LOCK.
000330     IF WS-STATUS = "00"
               MOVE ZERO         TO WS-F-ERROR
000340	       GO TO READ-DEBHIS-EXIT.
000350     IF (WS-STATUS = "23") OR
              (WS-STAT1 = "1")
000360	       MOVE 54		 TO WS-F-ERROR
000370	       GO TO READ-DEBHIS-EXIT.
000380     IF WS-STAT1 = "2" OR "3" OR "4"
000390	       MOVE 54		 TO WS-F-ERROR
000400         PERFORM READ-ERROR.
	     GO TO READ-DEBHIS-EXIT.

000310 READ-DEBHIS-NEXT-LOCK.
000320	     READ DEBHIS NEXT WITH KEPT LOCK.
000330     IF WS-STATUS = "00"
               MOVE ZERO         TO WS-F-ERROR
000340	       GO TO READ-DEBHIS-EXIT.
000350     IF (WS-STATUS = "23") OR
              (WS-STAT1 = "1")
000360	       MOVE 54		 TO WS-F-ERROR
000370	       GO TO READ-DEBHIS-EXIT.
000380     IF WS-STAT1 = "2" OR "3" OR "4"
000390	       MOVE 54		 TO WS-F-ERROR
000400         PERFORM READ-ERROR.
000410     IF RECORD-LOCKED
	       MOVE W02-DEBHIS	 TO WS-FILE
	       MOVE ZERO	 TO WS-KEY
000420	       PERFORM LOCKED-RECORD
000430	       GO TO READ-DEBHIS-NEXT-LOCK.
000250     IF TOO-MANY-LOCKS
000260	       UNLOCK DEBHIS
000270	       GO TO READ-DEBHIS-NEXT-LOCK.
	     GO TO READ-DEBHIS-EXIT.

000310 READ-DEBHIS-PREV.
000320	     READ DEBHIS PREVIOUS WITH IGNORE LOCK.
000330     IF WS-STATUS = "00"
               MOVE ZERO         TO WS-F-ERROR
000340	       GO TO READ-DEBHIS-EXIT.
000350     IF (WS-STATUS = "23") OR
	      (WS-STAT1 = "1" OR "4")
000360	       MOVE 54		 TO WS-F-ERROR
000370	       GO TO READ-DEBHIS-EXIT.
000380	   IF WS-STAT1 = "2" OR "3"
000390	       MOVE 54		 TO WS-F-ERROR
000400         PERFORM READ-ERROR.
	     GO TO READ-DEBHIS-EXIT.

