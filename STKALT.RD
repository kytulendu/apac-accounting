000020 READ-STKALT.
000030	     READ STKALT WITH IGNORE LOCK
	       KEY IS STKA-KEY.
000040     IF WS-STATUS = "00"
               MOVE ZERO         TO WS-F-ERROR
000050	       GO TO READ-STKALT-EXIT.
           IF WS-STATUS = "23"
	       MOVE 49		 TO WS-F-ERROR
000050	       GO TO READ-STKALT-EXIT.
000060     IF WS-STAT1 = "2" OR "3" OR "4"
000070	       MOVE 49		 TO WS-F-ERROR
000080         PERFORM READ-ERROR.
	     GO TO READ-STKALT-EXIT.

000150 READ-STKALT-LOCK.
000160	     READ STKALT WITH KEPT LOCK
	       KEY IS STKA-KEY.
000170     IF WS-STATUS = "00"
               MOVE ZERO         TO WS-F-ERROR
000180	       GO TO READ-STKALT-EXIT.
           IF WS-STATUS = "23"
	       MOVE 49		 TO WS-F-ERROR
000050	       GO TO READ-STKALT-EXIT.
000190     IF WS-STAT1 = "2" OR "3" OR "4"
000200	       MOVE 49		 TO WS-F-ERROR
000210         PERFORM READ-ERROR.
000220     IF RECORD-LOCKED
	       MOVE W02-STKALT	 TO WS-FILE
	       MOVE ZERO	 TO WS-KEY
000230         PERFORM LOCKED-RECORD
000240	       GO TO READ-STKALT-LOCK.
000250     IF TOO-MANY-LOCKS
000260	       UNLOCK STKALT
000270	       GO TO READ-STKALT-LOCK.
	     GO TO READ-STKALT-EXIT.

